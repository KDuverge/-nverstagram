<% include partials/header %>

<div class="form-setup">
  <form action="/gallery/<%= image._id %>/?_method=PUT" method="POST">
    <label for="name">Name</label>
    <input type="text" name="gram[name]" placeholder="Name" value="<%= image.name %>"><br>
    <label for="description">Description</label>
    <input type="text" name="gram[description]" placeholder="Description" value="<%= image.description %>"><br>
    <input type="submit" value="Submit">
  </form>
</div>


<% include partials/footer %>